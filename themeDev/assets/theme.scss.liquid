  }
  & tr{
    border-bottom: 1px solid #ececec;
    @include media-query($xsmall){
      border-bottom: none;
    }
  }
}

.cart-table-head{
  border-bottom: 1px solid #ececec;

  @include media-query($xsmall){
    border-bottom: none;
  }

  & th{
    padding: 8px 16px;
    font-family: "adobe-caslon-pro", Georgia, 'Times New Roman', Times, serif;
    font-weight: 400;
    font-size: 18px;
    @include media-query($xsmall){
      display: none;
    }
  }

  & th:first-child{
    padding-left: 0;
  }
}

.text-left{
  text-align: left;
}

.text-right{
  text-align: right;
}

.text-center{
  text-align: center;
}

.cart-item-image-wrapper{
  max-width: 100px;
}

.table-cart-image{
  max-width: 100px;
  
  &:hover{
    opacity: $link-hover-opacity;
  }
  @include media-query($xsmall){
    max-width: 56px;
  }
}

.cart-item-title{
  padding-right: 16px !important;
  padding-left: 16px !important;
  width: 408px;
  @include media-query($xsmall){
    padding-right: 8px !important;
    padding-left: 8px !important;
  }
}

.cart-item-price{
  text-align: right;
  padding-right: 16px !important;
  @include media-query($xsmall){
    display: none;
  }
}

.xsmall--show{
  display: none;
  @include media-query($xsmall){
    display: block;
  }
}

.xsmall--hide{
  display: block;
  @include media-query($xsmall){
    display: none;
  }
}

.cart-item-quantity{
  text-align: center;
}

.cart-item-tot-price{
  text-align: right;
  padding-right: 16px !important;
  @include media-query($xsmall){
    display: none;
  }
}

.cart-item-tot-rem{
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  @include flex-direction(column);
}

.cart-item-remove{
  margin-top: 16px;
  color: #C0363A;
  text-decoration: underline;

  &:hover{
    opacity: ($link-hover-opacity - 0.1);
  }
}

.xs-cart-item-remove{
  color: #C0363A;
  text-decoration: underline;
  display: none;
  &:hover{
    opacity: ($link-hover-opacity - 0.1);
  }
  @include media-query($xsmall){
    display: block;
  }
}

.cart-submit-wrapper{
  @include display-flexbox();
  @include flex-direction(column);
  margin-top: 64px;
}

.cart-total-sum-container{
  @include display-flexbox();
  @include flex-direction(row);
  @include justify-content(flex-end);
  margin-bottom: 8px;
}

.cart-flex-column{
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  @include flex-direction(column);
}

.cart-sum-text{
  margin-right: 16px;
  text-align: right;
}

.cart-checkout-btn{
