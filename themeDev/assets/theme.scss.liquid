  padding-left: 16px;
  padding-right: 16px;
  &:hover{
    opacity: $link-hover-opacity;
  }
}

.xsmall-title{
  font-size: 18px;
  line-height: 24px;
}

.xs-quanitty-input-wrapper{
  @include media-query($xsmall){
    display: -webkit-flex;
