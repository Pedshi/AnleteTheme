  justify-content: center;
  width: 48px;
  cursor: pointer;
  margin: 0;
  position: relative;
}

.product-quantity-input button:before,
.product-quantity-input button:after {
  display: inline-block;
  position: absolute;
  content: '';
  width: 0.5rem;
  height: 1px;
  background-color: #121212;
  transform: translate(-50%, -50%);
}
