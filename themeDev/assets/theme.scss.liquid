/*================ Detail slider ================*/

.detail-text-slider{
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  @include justify-content(space-around);
  width: 90%;
  min-width: 392px;
  border: 1px solid #4691B0;
  border-radius: 20px;
  height: 24px;
  margin-bottom: 8px;

  @media only screen and (max-width: 749px) and (min-width: 350px) {
    min-width: 200px !important;
    width: 100%;
    margin-left: -8px;
  }

  @media only screen and (max-width: 349px){
    @include flex-wrap(wrap);
    height: auto;
    min-width: auto;
  }

}

.detail-text-slider span{
  text-align: center;
  cursor: pointer;
  
  &:hover{
    text-decoration: underline;
  }
}

.small-slider-text{
  width: 75%;
}

.large-slider-text{
  width: 100%;
}

.active-text-slider{
  background-color: #d6e5eb;
  border-radius: 20px;
  &:hover{
    opacity: 1 !important;
  }
}


